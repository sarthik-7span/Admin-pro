{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      id: '',\n      username: '',\n      password: '',\n      localdataObj: null\n    };\n  },\n  methods: {\n    async Update() {\n      const setdata = await axios.put(\"http://localhost:3000/users\", {\n        id: this.id,\n        username: this.username,\n        password: this.password\n      });\n      console.log(setdata);\n    }\n  },\n  mounted() {\n    const localdata = JSON.parse(localStorage.getItem(\"user-info\")).data;\n    this.id = localdata.id;\n    this.username = localdata.username;\n    this.password = localdata.password;\n  }\n};","map":{"version":3,"names":["axios","data","id","username","password","localdataObj","methods","Update","setdata","put","console","log","mounted","localdata","JSON","parse","localStorage","getItem"],"sources":["src/components/TheUpdate.vue"],"sourcesContent":["<template>\n    <div>\n        <form @submit.prevent=\"Update\" class=\"login-form\">\n                <h2 class=\"title\">Update</h2>\n                <div class=\"field-wrap\">\n                        <label for=\"id\">Id:</label>\n                        <input type=\"text\" id=\"id\" v-model=\"id\" required>\n                    </div>\n                <div class=\"field-wrap\">\n                    <label for=\"username\">Username:</label>\n                    <input type=\"text\" id=\"username\" v-model=\"username\" required>\n                </div>\n                <div class=\"field-wrap\">\n                    <label for=\"password\">Password:</label>\n                    <input type=\"password\" id=\"password\" v-model=\"password\" required>\n                </div>\n                <div class=\"field-wrap\">\n                    <button type=\"submit\" class=\"button\">Update</button>\n                </div>\n            </form>\n    </div>\n</template>\n<script>\nimport axios from \"axios\";\n\nexport default {\n\n    data() {\n        return {\n            id: '',\n            username: '',\n            password: '',\n            localdataObj: null\n        }\n    },\n    methods: {\n        async Update() {\n            const setdata = await axios.put(\"http://localhost:3000/users\", {\n                id: this.id,\n                username: this.username,\n                password: this.password\n            })    \n            console.log(setdata)      \n        }  \n    },\n    mounted() {\n        const localdata = JSON.parse(localStorage.getItem(\"user-info\")).data;\n        this.id = localdata.id\n        this.username = localdata.username\n        this.password = localdata.password\n    }\n\n}\n</script>"],"mappings":"AAuBA,OAAAA,KAAA;AAEA;EAEAC,KAAA;IACA;MACAC,EAAA;MACAC,QAAA;MACAC,QAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,OAAA;MACA,MAAAC,OAAA,SAAAR,KAAA,CAAAS,GAAA;QACAP,EAAA,OAAAA,EAAA;QACAC,QAAA,OAAAA,QAAA;QACAC,QAAA,OAAAA;MACA;MACAM,OAAA,CAAAC,GAAA,CAAAH,OAAA;IACA;EACA;EACAI,QAAA;IACA,MAAAC,SAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA,eAAAhB,IAAA;IACA,KAAAC,EAAA,GAAAW,SAAA,CAAAX,EAAA;IACA,KAAAC,QAAA,GAAAU,SAAA,CAAAV,QAAA;IACA,KAAAC,QAAA,GAAAS,SAAA,CAAAT,QAAA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}