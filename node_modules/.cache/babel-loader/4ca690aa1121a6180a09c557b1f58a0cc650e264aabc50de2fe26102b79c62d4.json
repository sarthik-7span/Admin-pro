{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      username: '',\n      password: ''\n    };\n  },\n  methods: {\n    async signup() {\n      const signup = await axios.post(\"http://localhost:3000/users\", {\n        username: this.username,\n        password: this.password\n      });\n      console.log(this.username, signup);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","username","password","methods","signup","post","console","log"],"sources":["src/components/TheSignup.vue"],"sourcesContent":["<template>\n    <div>\n        <form @submit.prevent=\"signup\" class=\"login-form\">\n            <h2 class=\"title\">SignUp</h2>\n            <div class=\"field-wrap\">\n                <label for=\"username\">Username:</label>\n                <input type=\"text\" id=\"username\" v-model=\"username\" required>\n            </div>\n            <div class=\"field-wrap\">\n                <label for=\"password\">Password:</label>\n                <input type=\"password\" id=\"password\" v-model=\"password\" required>\n            </div>\n            <div class=\"field-wrap\">\n                <button type=\"submit\" class=\"button\">Signup</button>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n    data() {\n        return {\n            username: '',\n            password: '',\n        };\n    },\n    methods: {\n        async signup() {\n            const signup = await axios.post(\"http://localhost:3000/users\", {\n                username: this.username,\n                password: this.password,\n            })\n            console.log(this.username,signup)\n        }\n    }\n};\n</script>"],"mappings":"AAoBA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,OAAA;MACA,MAAAA,MAAA,SAAAL,KAAA,CAAAM,IAAA;QACAJ,QAAA,OAAAA,QAAA;QACAC,QAAA,OAAAA;MACA;MACAI,OAAA,CAAAC,GAAA,MAAAN,QAAA,EAAAG,MAAA;IACA;EAC